{
    "name": "unclecheese/silverstripe-graphql-forms",
    "description": "A POC for SilverStripe forms in graphql.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "silverstripe/graphql": "4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "UncleCheese\\GraphQLForms\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
